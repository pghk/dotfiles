{
  "editor.tabSize": 2,
  "editor.fontSize": 12,
  "editor.lineHeight": 20,
  "editor.fontFamily": "Operator Mono SSm, Liga Consolas, Menlo, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "Material Theme",
  "editor.minimap.enabled": false,
  "editor.fontLigatures": true,
  "workbench.iconTheme": "eq-material-theme-icons",
  "materialTheme.fixIconsRunning": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "window.zoomLevel": 0,
  "files.associations": {
    "*.inc": "php",
    "*.module": "php",
    "*.install": "php",
    "*.theme": "php",
    "*.tpl.php": "php",
    "*.test": "php",
    "*.php": "php"
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.sideBar.location": "left",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "gitlens.gitExplorer.location": "scm",
  "gitlens.historyExplorer.location": "scm",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.command": "gitlens.toggleFileBlame",
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "phpcs.standard": "Drupal",
  "phpcs.showSources": true,
  "search.location": "panel",
}