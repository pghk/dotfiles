{
  "title": "Modifier pairs for A360",
  "rules": [
    {
      "description": "Left ⌥⌘ from up+down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "up_arrow"
              },
              {
                "key_code": "down_arrow"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_option",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Left ⌃⌥ from ⌃up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "left_control"
              },
              {
                "key_code": "up_arrow"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_control",
              "modifiers": [
                "left_option"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Left ⌃⌘ from ⌃down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "left_control"
              },
              {
                "key_code": "down_arrow"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_control",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Right ⌥⌘ from left+right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "left_arrow"
              },
              {
                "key_code": "right_arrow"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "right_option",
              "modifiers": [
                "right_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
